
public class Time {

	public static void main(String[] args) {
		//to print seconds in hours and minutes
		int seconds = 7200;
		//int minutes = seconds/60;
		int hours = seconds/3600;
		int minutes = hours/60;
		if(minutes>60)
		{
			hours = hours + 1;
		}
		else {
			minutes = seconds/60;
		}
		System.out.println(hours + " hrs " + minutes + " mins ");
		
		
		
	}

}
