
public class Time {

	public static void main(String[] args) {
		//to print seconds in hours and minutes
		int seconds = 122;
		//to print seconds in minutes
		
		/*int minutes;
		minutes = seconds/60;
		System.out.println(minutes + "mins");*/
		
		//to print seconds in hours
		double hours;
		hours = seconds/3600;
		System.out.println(hours + "hrs");
		double minutes;
		minutes = hours/60;
		System.out.println(minutes);
		
		
	}

}
