import java.util.Random;
import java.util.*;
public class randomSum {

	public static void main(String[] args) {
		// enter 5 numbers and print the sum of random 2 numbers
		Random rand = new Random();
		Scanner input = new Scanner(System.in);
		System.out.println("Enter the range of numbers u want to randomly sum! ");
		int i = input.nextInt(); 
		int a = rand.nextInt(i);
		System.out.println(a);
		int b = rand.nextInt(i);
		System.out.println(b);
		int c = rand.nextInt(i);
		System.out.println(c);
		int d = rand.nextInt(i);
		System.out.println(d);
		int e = rand.nextInt(i);
		System.out.println(e);
		int sum = a + b + c + d + e;
		System.out.println("The sum is " + sum);
	}

}
