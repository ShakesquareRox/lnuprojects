package Practice;

public class cozaLozaWoza {

	public static void main(String[] args) {
		// coza for multiples of 3; loza for 5; woza for 7
		int i;
		for (i = 1; i < 11; i++) {
			if (i % 3 == 0) {
				System.out.println("\t coza");
				if (i % 5 == 0) {
					System.out.println("\t loza");
				}
				if (i % 7 == 0) {
					System.out.println("\t Woza");
				}
			} else {
				System.out.println(i);
			}
		}

	}

}
